#---------------Настройки APIs
APIs.warning=true                          #Отправлять сообщение если нет поддержки получения аудио от платформы
APIs.proxy=false                           #Делать запросы APi через прокси (Data/Json/Proxy.json) | username:password пока не работают

#Лимиты получаемых данных
APIs.limit.search=5                        #Кол-во треков в поиске
APIs.limit.author=5                        #Кол-во треков при запросе автора
APIs.limit.playlist=50                     #Кол-во треков при получении плейлиста или альбома
APIs.error.send=true                       #Отправлять ошибки в channel.error
APIs.error.show=true                       #Отправлять ошибки в консоль
#---------------Настройки APIs


#---------------Настройки FFmpeg
ffmpeg.name="ffmpeg"                      #Путь до FFmpeg, указать полный путь до файла
ffprobe.name="ffprobe"                    #Путь до FFprobe, указать полный путь до файла
#---------------Настройки FFmpeg


#---------------DEBUG MODE
debug.fs=false                             #Отслеживаем загрузки компонентов
debug.client=false                         #Отслеживаем клиент
debug.request=false                        #Отслеживаем httpsClient
debug.request.proxy=false                  #Отслеживаем httpsClient (необходима APIs.proxy=true)
debug.cycle=false                          #Отслеживаем циклы (resource/Structures/Cycles)
debug.ffmpeg=false                         #Отслеживаем FFmpeg
debug.player=false                         #Отслеживаем плеер
debug.player.audio=false                   #Отслеживаем аудио конвертер "OpusAudio"
debug.player.cache=false                   #Отслеживаем кеширование треков
#---------------DEBUG MODE


#---------------Настройки музыки
music.info=true                            #Бот будет показывать сделанный запрос
music.leave=false                          #Бот будет отключатся по окончанию проигрывания треков

#Настройки кеширования
music.cache.enable=false                   #Бот будет кешировать треки
music.cache.dir="Audio"                    #Путь куда будет кешироваться музыка

#Настройки плеера
music.player.duration=20                   #Время обновления плееров
music.player.message=20                    #Время обновления сообщений
music.player.timeout=2                     #Время через которое будет включен следующий трек, можно использовать float значения (1.5). Не влияет на первый трек

#Настройки очереди
music.queue.destroy=10                     #Время для авто удаления очереди

#Настройки аудио
music.audio.fade=4                         #Длительность плавного включения
music.audio.volume=100                     #Громкость аудио
music.audio.bitrate="128K"                 #Битрейт аудио, не ставить выше 256k
music.audio.type="opus"                    #Тип аудио (opus, raw)

#Картинка note, можно заменить
music.note="https://raw.githubusercontent.com/SNIPPIK/WatKLOK/main/.github/resource/Icons/Note.png"

