{
  "name": "watklok",
  "version": "4.0.0",
  "description": "Music discord bot",
  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "bufferutil": "^4.0.7",
    "discord.js": "^14.13.0",
    "dotenv": "^16.0.3",
    "sodium-native": "^4.0.4",
    "utf-8-validate": "^6.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "start": "cd .Build && node -r tsconfig-paths/register src/app.js",
    "shard": "cd .Build && node -r tsconfig-paths/register src/app.js --ShardManager",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -watch"
  }
}
